. list_str

f1(){
    list_make a
    O=a
    O=a list_push a b
    O=a list_print

    echo "------------------"
    O=a list_unshift 1 2
    O=a list_print
    echo "------------------"
    O=a echo "$(list_tail)"
    echo
    echo "----------"

    time list_head
}

f2(){
    list_make a
    O=a
    O=a list_push a b
    O=a list_unshift 1 2
    echo "------------------ remove"
    O=a list_tail
    O=a list_remove 3
    O=a list_tail

    echo "------------------ shift"
    O=a echo "$(list_head)"
    O=a echo "--> |$(list_shift)|"
    O=a echo "$(list_head)"
}

test_remove(){
    list_make a
    O=a
    list_push $(seq 10)
    time list_remove 3
    list_print
}

# test_remove

test_regex(){
    list_make a; O=a
    list_push a1 b2 c3
    list_get_by_regex "[a-z]2"
}

test_regex

test_index_of(){
    list_make a; O=a
    list_push a1 b2 c3
    list_index_of "c3"
}

test_index_of

